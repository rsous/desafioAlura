x;\n  align-items: center;\n  justify-content: space-between;\n\n  &__logo {\n    width: 20%;\n  }\n\n  &__user {\n    width: 20%;\n  }\n\n  .logo {\n    height: 3.4rem;\n    width: 14.5rem;\n  }\n\n  &__form {\n    margin: 0 3rem;\n    width: 60%;\n  }\n\n  .search {\n    color: $color-text;\n    font-size: $font-size-default;\n    height: 5.6rem;\n    width: 100%;\n\n    padding: 2rem;\n    background: $color-input-bg;\n    border-radius: 8px;\n    border: none;\n    outline: none;\n    transition: all 0.2s ease;\n\n    &:hover,\n    &:focus {\n      background: $color-input-bg-hover;\n    }\n  }\n\n  .user {\n    display: flex;\n    align-items: center;\n    justify-content: flex-end;\n    padding: 1rem 2rem;\n    margin-left: auto;\n    max-width: 14.5rem;\n\n    &-name {\n      color: $color-text;\n      font-size: $font-size-default;\n      font-weight: lighter;\n      margin-left: 1rem;\n    }\n\n    &:hover {\n      background: rgba(#fff, 0.08);\n      border-radius: 8px;\n      cursor: pointer;\n    }\n  }\n}\n","scss/layout/_menu.scss":"@use 'abstracts' as *;\n\n//FIXME\n.content {\n  display: flex;\n  justify-content: space-between;\n  margin-top: 5rem;\n}\n\n.menu {\n  display: flex;\n  flex-direction: column;\n  width: 20%;\n\n  & > *:not(:last-child) {\n    margin-bottom: 2rem;\n  }\n\n  &__title {\n    font-size: 1.2rem;\n    text-transform: uppercase;\n    font-weight: lighter;\n  }\n\n  &__item {\n    display: flex;\n    align-items: center;\n\n    &-name {\n      font-size: 1.6rem;\n    }\n\n    &:hover {\n      cursor: pointer;\n    }\n\n    &:hover &-name {\n      opacity: 0.8;\n    }\n\n    &:hover .menu__icon {\n      background: rgba($color-tertiary, 0.64);\n    }\n  }\n\n  &__icon {\n    background: $color-tertiary;\n    border-radius: 1.6rem;\n    margin-right: 1rem;\n    padding: 1.2rem;\n\n    &.comunidade {\n      opacity: 0.16;\n    }\n  }\n}\n","scss/layout/_editor.scss":"@use 'abstracts' as *;\n\n.editor {\n  display: flex;\n  flex-dir