 2rem;\n  background: rgba($color-tertiary, 0.08);\n  border-radius: 0.8rem;\n  border: none;\n  transition: all 0.2s ease;\n\n  &:hover {\n    background: rgba($color-tertiary, 0.16);\n    cursor: pointer;\n  }\n}\n","scss/base/_typography.scss":"@use 'abstracts' as *;\n\n.body {\n  color: $color-text;\n  font-family: 'Inter', sans-serif;\n}\n","scss/abstracts/_variables.scss":"$darkred: darkred;\n$color-primary: #051d3b;\n$color-secondary: #6bd1ff;\n$color-tertiary: #5081fb;\n$color-text: #fff;\n$color-input-bg: rgba(255, 255, 255, 0.16);\n$color-input-bg-hover: rgba(255, 255, 255, 0.24);\n\n$font-size-default: 1.6rem;\n$font-family: 'Inter', sans-serif;\n$font-size-code: 1.4rem;\n$font-family-code: 'Roboto mono', monospace;\n","scss/components/_input.scss":"@use 'abstracts' as *;\n\n.input {\n  color: $color-text;\n  font-size: $font-size-default;\n  font-family: $font-family;\n  height: 5.6rem;\n  width: 100%;\n\n  padding: 1.5rem;\n  background: $color-input-bg;\n  border-radius: 8px;\n  border: none;\n  outline: none;\n  transition: all 0.2s ease;\n\n  &:hover,\n  &:focus {\n    background: $color-input-bg-hover;\n  }\n}\n","scss/components/_titles.scss":"@use 'abstracts' as *;\n\n.title {\n  letter-spacing: 0.6rem;\n}\n","scss/layout/_header.scss":"@use 'abstracts' as *;\n\n.header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n\n  &__logo {\n    width: 20%;\n  }\n\n  &__user {\n    width: 20%;\n  }\n\n  .logo {\n    height: 3.4rem;\n    width: 14.5rem;\n  }\n\n  &__form {\n    margin: 0 3rem;\n    width: 60%;\n  }\n\n  .search {\n    color: $color-text;\n    font-size: $font-size-default;\n    height: 5.6rem;\n    width: 100%;\n\n    padding: 2rem;\n    background: $color-input-bg;\n    border-radius: 8px;\n    border: none;\n    outline: none;\n    transition: all 0.2s ease;\n\n    &:hover,\n    &:focus {\n      background: $color-input-bg-hover;\n    }\n  }\n\n  .user {\n    display: flex;\n    align-items: center;\n    justify-content: flex-end;\n    padding: 1rem 2rem;\n    margin-left: auto;\n    max-width: 14.5rem;\n\n    &-name {\n      color: $color-text;\n      font-size: $font-size-default;\n      font-weight: lighter;\n      margin-left: 1rem;\n    }\n\n    &:hover {\n      background: rgba(#fff, 0.08);\n      border-radius: 8px;\n      cursor: pointer;\n    }\n  }\n}\n","scss/layout/_menu.scss":"@use 'abstracts' as *;\n\n//FIXME\n.content {\n  display: flex;\n  justify-content: space-between;\n  margin-top: 5rem;\n}\n\n.menu {\n  display: flex;\n  flex-direction: column;\n  width: 20%;\n\n  & > *:not(:last-child) {\n    margin-bottom: 2rem;\n  }\n\n  &__title {\n    font-size: 1.2rem;\n    text-transform: uppercase;\n    font-weight: lighter;\n  }\n\n  &__item {\n    display: flex;\n    align-items: center;\n\n    &-name {\n      font-size: 1.6rem;\n    }\n\n    &:hover {\n      cursor: pointer;\n    }\n\n    &:hover &-name {\n      opacity: 0.8;\n    }\n\n    &:hover .menu__icon {\n      background: rgba($color-tertiary, 0.64);\n    }\n  }\n\n  &__icon {\n    background: $color-tertiary;\n    border-radius: 1.6rem;\n    margin-right: 1rem;\n    padding: 1.2rem;\n\n    &.comunidade {\n      opacity: 0.16;\n    }\n  }\n}\n","scss/layout/_editor.scss":"@use 'abstracts' as *;\n\n.editor {\n  display: flex;\n  flex-direction: column;\n  margin: 0 3rem;\n  width: 60%;\n\n  &__container {\n    background: $color-secondary;\n    border-radius: 0.8rem;\n    margin-bottom: 3rem;\n    padding: 3rem;\n    position: relative;\n  }\n\n  &__code {\n    background-color: #000;\n    border: none;\n    border-radius: 8px;\n    color: $color-text;\n    font-size: $font-size-code;\n    font-family: $font-family-code;\n    padding: 5rem 2rem 2rem;\n    resize: none;\n    width: 100%;\n\n    &:focus {\n      outline: none;\n    }\n  }\n\n  &__buttons {\n    position: absolute;\n    top: 4.5rem;\n    left: 4.5rem;\n\n    &:hover {\n      cursor: pointer;\n    }\n  }\n}\n","scss/layout/_project.sc